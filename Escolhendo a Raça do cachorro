<body>
    <img src="" alt="">
    <script>
        let raca = prompt("Digite uma raÃ§a: ")
        fetch(`https://dog.ceo/api/breed/${raca}/images/random`)
        .then(resposta => resposta.json())
        .then(dados =>{
            let imagem = document.querySelector("img")
            imagem.src = dados.message
        } )
    </script>
</body>
